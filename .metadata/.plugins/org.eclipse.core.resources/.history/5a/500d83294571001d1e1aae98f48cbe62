package exampletest;

public class AnonyMousExample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Anonymous anony = new Anonymous();
		anony.field.start();
		anony.metho1();
		anony.method2(
				new Worker () {
				public void start() {
					System.out.println("테스트를 합니다.");
				}	
				}
				);
	}

}
